    <section class="form">
      <div class="container my-4">
        <div class="row g-0 shadow top">
          <div class="d-none d-lg-inline-block col-lg-6 mid">
            <img src="/images/register.jpg" alt="" class="img-fluid img" />
          </div>
          <div class="col-lg-6 px-5 pt-4 bottom">
            <h1 class="font-weight-bold brand-name mb-4">Brand name</h1>

            <form
              action="/admin/register"
              class="mx-2 validated-form form password-strength"
              novalidate
              method="POST"
            >
              <h3 class="mb-4 create-account">Create a new account</h3>
              <div class="form-row mb-2">
                <label class="form-label" for="name" class="form-label"
                  >Owner Name</label
                >
                <div class="input-group col-lg-12">
                  <span class="icons input-group-addon"
                    ><i class="fas fa-user"></i
                  ></span>
                  <input
                    autocomplete="off"
                    type="text"
                    id="name"
                    name="name"
                    placeholder="Owner Name"
                    class="form-control input validate-me"
                    spellcheck="false"
                    
                    required
                  />
                  <div class="valid-feedback">Looks good</div>
                </div>
              </div>
              <div class="form-row mb-2">
                <label class="form-label" for="Store_name" class="form-label"
                  >Store Name</label
                >
                <div class="col-lg-12 input-group">
                  <span class="icons input-group-addon"
                    ><i class="fas fa-store"></i
                  ></span>
                  <input
                    autocomplete="off"
                    type="text"
                    name="store_name"
                    id="Store_name"
                    placeholder="Store name"
                    class="form-control input validate-me"
                    spellcheck="false"
                    
                    required
                  />
                  <div class="valid-feedback" id="store-name-feedback">Looks good</div>
                </div>
              </div>

              <div class="role-select">
                <label class="form-label" for="role" class="form-label"
                >Role</label
              >
                <select  name="role" class="form-select " id="role" aria-label="Default select example">
                  <!-- <option selected >Select role</option> -->
                  <option value="drop-shipper" selected>Drop Shipper</option>
                  <option  value="whole-seller">Whole Seller</option>
                  <!-- <option  value="shop-owner">Shop Owner</option> -->
                </select>
              </div>
              <div class="form-row mb-2">
                <label class="form-label" for="email " class="form-label"
                  >Email</label
                >
                <div class="col-lg-12 input-group">
                  <span class="icons input-group-addon"
                    ><i class="fas fa-envelope"></i
                  ></span>
                  <input
                    autocomplete="off"
                    onkeyup="emailCheck(this.value)"
                    type="email"
                    name="email"
                    id="email"
                    placeholder="Enter your Email id"
                    class="form-control input validate-me"
                    spellcheck="false"
                    required
                  />
                </div>
                <span class="email-check"></span>
              </div>
              <div class="form-row mb-2">
                <label class="form-label" for="Password " class="form-label"
                  >Password</label
                >
                <div class="col-lg-12 input-group">
                  <span class="icons input-group-addon"
                    ><i class="fas fa-lock"></i
                  ></span>
                  <input
                    autocomplete="off"
                    type="password"
                    name="password"
                    id="Password"
                    placeholder="Password"
                    class="form-control shadow-5 input "
                    required
                    onkeyup="passCheck(this.value)"
                    
                  />
                  <!-- <div class="valid-feedback">
                                Looks good
                              </div> -->
                </div>
                <div class="password-param">
                  <p>
                    <label class="password-check">minimum 8 characters</label>
                    <label class="password-check">1 lower case character</label>
                    <label class="password-check">1 upper character</label>
                    <label class="password-check">1 numerical character</label>
                    <label class="password-check">1 special character</label>
                  </p>
                </div>
              </div>

              <div class="register_btn">
                <button class="btn btn-reg my-1">Register</button>
                
                
              </div>
            </form>
            <div class="text-end">
              <!-- <button class="btn-xs btn-primary text-end btn-log my-2 ">Login</button> -->

              <p class="login_link">
                Already have an account ?
                <a href="http://localhost:3000/admin/login" type="submit">
                  Login</a
                >
              </p>
            </div>
          </div>
        </div>
      </div>
    </section>

   

